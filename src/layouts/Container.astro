---
import CustomContainer from "@components/Container";
import BaseLayoutWithNavBar from "./BaseLayoutWithNavBar.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayoutWithNavBar title={title}>
  <CustomContainer
    className="flex flex-col relative min-h-[calc(100%-64px)]"
    as="main"
  >
    <slot />
  </CustomContainer>
</BaseLayoutWithNavBar>
