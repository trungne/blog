---
import Container from "@components/Container"
import BaseLayoutWithNavBar from "./BaseLayoutWithNavBar.astro"

export interface Props {
  title: string
}

const { title } = Astro.props
---

<BaseLayoutWithNavBar title={title}>
  <Container
    className="flex flex-col relative min-h-[calc(100vh-64px)]"
    as="main"
  >
    <slot />
  </Container>
</BaseLayoutWithNavBar>
